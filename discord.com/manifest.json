{
	"manifest_version":2,
	"name":"Discord",
	"short_name":"Disc",
	"description":"Embedded Chrome App for Discord.",
	"version":"1.0",
	"scope":"./discord.com/",
	"start_url": "./discord.com/html/embed.html?source=pwa",
	"display": "standalone",
	"icons":[
		{
			"src": "img/icon.png",
			"sizes": "32x32",
			"type": "image/png"
		},
		{
			"src": "img/icon128.png",
			"sizes": "128x128",
			"type": "image/png"
		},
		{
			"src": "img/icon192.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "img/icon512.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	],
	"app":{
		"background":{
			"scripts":[
				"js/background.js"
			],
			"pages":[
				"html/embed.html"
			]
		}
	},
	"permissions":[
		"webview",
		"fullscreen",
		"notifications",
		"audioCapture",
		"videoCapture"
	]
}
